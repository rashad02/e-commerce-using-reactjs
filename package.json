{
    "name": "daily-solution-server",
    "version": "1.0.0",
    "engines": {
        "node": "v10.20.1",
        "npm": ""
    },
    "proxy": "http://localhost:5000",
    "scripts": {
        "client": "cd client && npm start",
        "server": "cd server && node server.js",
        "build": "./node_modules/.bin/netlify-lambda build src && cd client && npm run build",
        "dev": "concurrently --kill-others-on-fail \"npm server\" \"npm client\"",
        "start": "./node_modules/.bin/netlify-lambda serve src",
        "netlify-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.4",
        "axios": "^1.3.5",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "bootstrap": "^5.2.3",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "firebase": "^9.19.1",
        "js": "^0.1.0",
        "moment": "^2.29.4",
        "mongodb": "^5.8.1",
        "mongoose": "^7.0.3",
        "netlify-lambda": "^2.0.16",
        "node-sass": "^8.0.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.7.4",
        "react-datepicker": "^4.11.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.10.0",
        "react-scripts": "5.0.1",
        "react-stripe-checkout": "^2.6.3",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "reselect": "^4.1.8",
        "serverless-http": "^3.2.0",
        "stripe": "12.1.1"
    },
    "devDependencies": {
        "concurrently": "^8.0.1"
    }
}
